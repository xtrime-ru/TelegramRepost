{
  "name": "xtrime-ru/telegramrepost",
  "description": "Repost messages",
  "type": "project",
  "license": "MIT",
  "keywords": ["telegram", "parser", "repost"],
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/xtrime-ru/MadelineProto.git"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "ext-json": "*",
    "ext-pcntl": "*",
    "ext-mbstring": "*",
    "danog/madelineproto":"dev-v8-tas",
    "danog/async-orm": "^1.1",
    "vlucas/phpdotenv": "^5",
    "amphp/pipeline": "^1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Alexander Pankratov",
      "email": "alexander@i-c-a.su"
    }
  ],
  "autoload": {
    "psr-4": {
      "TelegramRepost\\": "app/"
    }
  }
}
